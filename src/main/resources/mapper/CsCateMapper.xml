<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.library.bookhub.repository.cs.CsCateRepository">

	<!-- cs cate1 리스트 조회 -->
	<select id="selectCsCate1" resultType="com.library.bookhub.entity.cs.CsCate1Entity">
		SELECT * FROM bh_cs_cate1 WHERE `cate1` >= 10
	</select>

	<!-- cs cate2 조회 , cate1로 조회해야 연관된 cate2 리스트가 조회됨-->
	<select id="selectCsCate2" resultType="com.library.bookhub.entity.cs.CsCate2Entity">
		SELECT * FROM bh_cs_cate2 WHERE `cate1` = #{cate1}
	</select>
	

</mapper>